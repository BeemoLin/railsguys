<h2>Hello Arduino<h2>
<div ng-app="ArduinoApp">
  <div ng-controller="IndexCtrl">
    <h1>{{ title }}</h1>
  </div>

  <div ng-controller="ArduinoCtrl" class="container">
    <button ng-click="setServo(5, 0)" class="btn btn-default">MoveServo(0)</button>
    <button ng-click="setServo(5, 180)" class="btn btn-default">MoveServo(180)</button>
  </div>
</div>

